{
  "manifest_version": 2,

  "name": "Remind2Visit",
  "description": "Reminds you to visit your favorite sites regularly",
  "version": "1.0",

  "permissions": [
    "storage"
  ],

  "browser_action": {
    "default_title": "Remind2Visit",
    "default_icon": "images/icon.png",
    "default_popup": "html/popup.html"
  }
}
